generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          String   @id @default(uuid())
  name        String
  description String?
  category    String
  quantity    Int
  price       Decimal
  supplier    String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
